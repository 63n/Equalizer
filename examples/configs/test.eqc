server
{
   config
   {
       appNode
       {
           pipe
           {
               window
               {
                   name "Clustered csg - powered by Equalizer"
                   viewport [ 100 400 480 300 ]
                   channel
                   {
                       name "channel1"
                   }
               }
               window
               {
                   name "Clustered csg - powered by Equalizer"
                   viewport [ 580 400 480 300 ]
                   channel
                   {
                       name "channel2"
                   }
               }
               window
               {
                   name "Clustered csg - powered by Equalizer"
                   viewport [ 100 100 480 300 ]
                   channel
                   {
                       name "channel3"
                   }
               }
               window
               {
                   name "Clustered csg - powered by Equalizer"
                   viewport [ 580 100 480 300 ]
                   channel
                   {
                       name "channel4"
                   }
               }
           }
       }
       compound
       {
           compound
           {
               channel "channel1"
               wall
               {
                   bottom_left  [ -.48 -.3 -1 ]
                   bottom_right [ -.0 -.3 -1 ]
                   top_left     [ -.48  .0 -1 ]
               }
           }
           compound
           {
               channel "channel2"
               wall
               {
                   bottom_left  [  .0 -.3 -1 ]
                   bottom_right [  .48 -.3 -1 ]
                   top_left     [  .0  .0 -1 ]
               }
           }
           compound
           {
               channel "channel3"
               wall
               {
                   bottom_left  [ -.48  .0 -1 ]
                   bottom_right [  .0   .0 -1 ]
                   top_left     [ -.48  .3 -1 ]
               }
           }
           compound
           {
               channel "channel4"
               wall
               {
                   bottom_left  [  .0   .0 -1 ]
                   bottom_right [  .48  .0 -1 ]
                   top_left     [  .0   .3 -1 ]
               }
           }
       }
   }
}

