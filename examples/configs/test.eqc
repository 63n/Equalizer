	# two-to-one sort-first config for cluster at unizh
	global
	{
		EQ_WINDOW_IATTR_HINT_FULLSCREEN off
	}

	server
	{
		config
		{
			latency 2
			appNode
			{
				pipe 
				{
					window
					{
						viewport [ 80 200 480 300 ]
						channel { name "channel1" }
					}
				}
			}
			node
			{
				connection { hostname "wolf1" }
				pipe { 
					window { 
						viewport [ 720 200 480 300 ]
						channel { name "channel2" }
					}
				}
			}
			compound
			{
				compound
				{
					channel "channel1"
					wall
					{
						bottom_left  [ -.64 -.2 -.75 ]
						bottom_right [  .0  -.2 -.75 ]
						top_left     [ -.64  .2 -.75 ]
					}
					outputframe{ name "frame" }
				}
				compound
				{
					task [ CLEAR ASSEMBLE ]
					channel "channel2"
					wall
					{
						bottom_left  [ .0 -.2 -.75 ]
						bottom_right [ .64 -.2 -.75 ]
						top_left     [ .0  .2 -.75 ]
					}
					inputframe{ name "frame" }
				}
			}
		}    
	}
