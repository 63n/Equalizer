# one-pipe fullscreen config, covering a 30" LCD one meter in front
# of the viewer

server
{
    config
    {
        latency 0
        appNode
        {
            pipe
            {
                window
                {
                    channel
                    {
                        name "channel"
                    }
                }
            }
        }
        compound
        { 
            channel "channel"
            wall
            {
                bottom_left  [ -.32 -.20 -.75 ]
                bottom_right [  .32 -.20 -.75 ]
                top_left     [ -.32  .20 -.75 ]
            }
        }
    }    
}
