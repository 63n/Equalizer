#Equalizer 1.1 ascii

global
{
#    EQ_CONFIG_FATTR_EYE_BASE 1
}

server
{
    connection { hostname "127.0.0.1" }
    config
    {
        appNode
        {
            pipe
            {
                window
                {
                    name "left"
                    viewport [ 100 100 1200 750 ]
                    channel
                    {
                        viewport [ 0 0 .5 .5 ]
                        name "channel1"
                    }
                    channel
                    {
                        viewport [ .5 0 .5 .5 ]
                        name "channel2"
                    }
                    channel
                    {
                        viewport [ 0 .5 .5 .5 ]
                        name "channel3"
                    }
                    channel
                    {
                        viewport [ .5 .5 .5 .5 ]
                        name "channel4"
                    }
                }
            }
        }
        observer {}
        layout { view { observer 0 mode STEREO }}
        canvas
        {
            layout 0
            segment
            { 
                channel "channel1"
                wall
                {
                    bottom_left  [ -.32 -.2 -1 ]
                    bottom_right [  .32 -.2 -1 ]
                    top_left     [ -.32  .2 -1 ]
                }
            }
            segment
            { 
                channel "channel2"
                wall
                {
                    bottom_left  [ -.32 -.2 -2 ]
                    bottom_right [  .32 -.2 -2 ]
                    top_left     [ -.32  .2 -2 ]
                }
            }
            segment
            { 
                channel "channel3"
                wall
                {
                    bottom_left  [ -.64 -.2 -2 ]
                    bottom_right [  .0  -.2 -2 ]
                    top_left     [ -.64  .2 -2 ]
                }
            }
            segment
            { 
                channel "channel4"
                wall
                {
                    bottom_left  [  .0  -.2 -2 ]
                    bottom_right [  .64 -.2 -2 ]
                    top_left     [  .0   .2 -2 ]
                }
            }
        }
    }    
}
