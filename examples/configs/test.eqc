# two-to-one sort-first config for cluster at unizh
global
{
    EQ_WINDOW_IATTR_HINT_FULLSCREEN off
}

server
{
    config
    {
        latency 20
        node
        {
            pipe 
            {
#                attributes{ hint_thread off }
                window
                {
                    viewport [ 80 200 480 300 ]
                    channel { name "channel1" }
                }
            }
        }
        appNode
        {
            pipe { 
                attributes{ hint_thread off }
                window { 
                    viewport [ 720 200 480 300 ]
                    channel { name "channel2" }
                }
            }
        }
        compound
        {
            compound
            {
                channel "channel1"
                wall
                {
                    bottom_left  [ -.64 -.2 -.75 ]
                    bottom_right [  .0  -.2 -.75 ]
                    top_left     [ -.64  .2 -.75 ]
                }
                swapbarrier{}
            }
            compound
            {
                channel "channel2"
                wall
                {
                    bottom_left  [ .0 -.2 -.75 ]
                    bottom_right [ .64 -.2 -.75 ]
                    top_left     [ .0  .2 -.75 ]
                }
                swapbarrier{}
            }
        }
    }    
}
